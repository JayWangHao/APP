<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cattsoft.wow.fragment.AreaChartQianFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="-10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/overview_view_page">


        <ImageView
            android:id="@+id/rank"
            android:layout_width="20dp"
            android:layout_height="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="30dp"
            android:background="@color/green"

            />

        <TextView
            android:id="@+id/ranktext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="27dp"
            android:layout_toRightOf="@id/rank"
            android:text="排名"
            android:textColor="@color/black"
            android:textSize="10sp"

            />

        <ImageView
            android:id="@+id/woNum"
            android:layout_width="20dp"
            android:layout_height="5dp"
            android:layout_below="@id/rank"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@color/red"

            />

        <TextView
            android:id="@+id/wotext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ranktext"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/woNum"
            android:text="工单数"
            android:textColor="@color/black"
            android:textSize="10sp" />
        <RelativeLayout
            android:layout_marginLeft="49dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


                <ImageView
                    android:id="@+id/iv_shizhu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:layout_marginTop="-12dp"
                    android:src="@drawable/shizhu" />

                <TextView

                    android:id="@+id/tv_shizhu_ranking_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="35dp"

                    android:text="18"
                    android:textColor="@color/green"

                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tv_shizhu_work_num"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/redpint"
                    android:gravity="center"
                    android:text="8888"
                    android:textColor="@color/white"
                    android:textSize="8sp" />



            <ImageView
                android:id="@+id/iv_qianjiang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="105dp"
                android:layout_marginTop="48dp"
                android:src="@drawable/qianjiang" />

            <TextView
                android:id="@+id/tv_qianjiang_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="130dp"
                android:layout_marginTop="95dp"

                android:text="18"
                android:textColor="@color/green"

                android:textSize="10sp" />


            <TextView
                android:id="@+id/tv_qianjiang_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="70dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />


            <ImageView
                android:id="@+id/iv_pengshui"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="87dp"
                android:src="@drawable/pengshui" />


            <TextView
                android:id="@+id/tv_pengshui_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="80dp"
                android:layout_marginTop="145dp"

                android:text="18"
                android:textColor="@color/green"

                android:textSize="10sp" />


            <TextView
                android:id="@+id/tv_pengshui_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="80dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />

            <ImageView
                android:id="@+id/iv_youyang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="154dp"
                android:src="@drawable/jiuyang" />

            <TextView
                android:id="@+id/tv_youyang_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="215dp"

                android:text="18"
                android:textColor="@color/green"

                android:textSize="10sp" />


            <TextView
                android:id="@+id/tv_youyang_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="210dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />


            <ImageView
                android:id="@+id/iv_xiushan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="227dp"
                android:src="@drawable/xiushan" />

            <TextView
                android:id="@+id/tv_xiushan_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="145dp"
                android:layout_marginTop="238dp"

                android:text="18"
                android:textColor="@color/green"

                android:textSize="10sp" />


            <TextView
                android:id="@+id/tv_xiushan_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="138dp"
                android:layout_marginTop="250dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />
        </RelativeLayout>
    </RelativeLayout>

</FrameLayout>
