<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cattsoft.wow.fragment.AreaChartPeiFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="-10dp"
        android:background="@drawable/overview_view_page">
        <RelativeLayout
            
            android:layout_marginLeft="15dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/rank"
                android:layout_width="20dp"
                android:layout_height="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                android:background="@color/green"

                />

            <TextView
                android:id="@+id/ranktext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="27dp"
                android:layout_toRightOf="@id/rank"
                android:text="排名"
                android:textColor="@color/black"
                android:textSize="10sp"

                />

            <ImageView
                android:id="@+id/woNum"
                android:layout_width="20dp"
                android:layout_height="5dp"
                android:layout_below="@id/rank"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:background="@color/red"

                />

            <TextView
                android:id="@+id/wotext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/ranktext"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="3dp"
                android:layout_toRightOf="@id/woNum"
                android:text="工单数"
                android:textColor="@color/black"
                android:textSize="10sp" />
            <ImageView
                android:id="@+id/iv_dianjiang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:src="@drawable/dianjiang" />

            <TextView
                android:id="@+id/tv_dianjiang_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="115dp"
                android:layout_marginTop="40dp"
                android:text="18"
                android:textColor="@color/green"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/tv_dianjiang_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="17dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />





            <ImageView
                android:id="@+id/iv_fengdu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="134dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/fengdu" />

            <TextView
                android:id="@+id/tv_fengdu_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="175dp"
                android:layout_marginTop="100dp"
                android:text="18"
                android:textColor="@color/green"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/tv_fengdu_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="170dp"
                android:layout_marginTop="80dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />

            <ImageView
                android:id="@+id/iv_peiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="63dp"
                android:layout_marginTop="85dp"
                android:src="@drawable/peiling" />



            <TextView
                android:id="@+id/tv_peiling_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="126dp"
                android:text="18"
                android:textColor="@color/green"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/tv_peiling_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />

            <ImageView
                android:id="@+id/iv_wulong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="103dp"
                android:layout_marginTop="138dp"
             android:background="@drawable/wulong" />

            <TextView
                android:id="@+id/tv_wulong_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="190dp"
                android:text="18"
                android:textColor="@color/green"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/tv_wulong_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="163dp"
                android:layout_marginTop="170dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />






            <ImageView
                android:id="@+id/iv_nanchuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="57dp"
                android:layout_marginTop="162dp"
                android:src="@drawable/nanchuan" />




            <TextView
                android:id="@+id/tv_nanchuan_ranking_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="83dp"
                android:layout_marginTop="210dp"
                android:text="18"
                android:textColor="@color/green"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/tv_nanchuan_work_num"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="190dp"
                android:background="@drawable/redpint"
                android:gravity="center"
                android:text="8888"
                android:textColor="@color/white"
                android:textSize="8sp" />



        </RelativeLayout>





    </RelativeLayout>
</FrameLayout>
